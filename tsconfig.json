{
    "extends": "@tsconfig/recommended/tsconfig.json",
    "files": ["func.ts"],
    "compilerOptions": {
        "target": "ES2021",
        "module": "commonjs",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    }
}
